.page-header
  %h2 Nueva Comision
= form_for [:admin, @commission], :html => {:multipart => true} do |form|
  = render "admin/shared/error_messages", :target => @commission
  %fieldset
    = render :partial => 'form', :locals => {:form => form}
    .actions
      = form.submit 'Crear Comision', :class => 'btn primary'
      %input{:type => 'reset', :class => 'btn', :value => 'Limpiar'}